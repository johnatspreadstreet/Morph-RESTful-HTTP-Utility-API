{
  "name": "Morph",
  "alias": "morph.now.sh",
  "version": 2,
  "builds": [
    { "src": "*.py", "use": "@now/python" , "config": { "maxLambdaSize": "10mb" } },
    { "src": "api/strings/*.py", "use": "@now/python" , "config": { "maxLambdaSize": "10mb" } }
  ],
  "routes": [
    { "src": "/strings/capitalize", "dest": "apis/strings/capitalize.py" }
  ]
}